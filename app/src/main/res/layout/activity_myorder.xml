<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_myorder"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.inwhoop.qscx.qscxsj.activitys.main.MyOrderActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/title_activity_authentication_inc"
            layout="@layout/title_base" />

        <ListView
            android:id="@+id/lv_myOrder"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/white_color">

        </ListView>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
